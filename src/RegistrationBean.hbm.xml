<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>  
  <class name="com.cdac.model.RegistrationBean" table="Voting_list">  
    <id name="Aadhar">  
     <generator class="assigned"></generator>  
    </id>  
       <property name="Status" type="string" length="50" not-null="true" column="Status"></property>      
    <property name="uname" type="string" length="50" not-null="true" column="Name"></property> 
     <property name="fname" type="string" length="50" not-null="true" column="Fathers_Name"></property>
      <property name="address" type="string" length="500" not-null="true" column="Address"></property>
       <property name="uemail" type="string" length="100" not-null="true" column="Email"></property>
        <property name="udob" type="string" length="15" not-null="true" column="Date_of_Birth"></property>
         <property name="ucontact" type="string" length="10" not-null="true" column="Phone_Number"></property> 
    <property name="Password" type="string" length="255" not-null="true"></property>  
     <property name="gender" type="string" length="255" not-null="true"></property>  
            
  </class>  
            
 </hibernate-mapping> 